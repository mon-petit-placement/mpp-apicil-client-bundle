{
    "name": "mpp/apicil-client-bundle",
    "type": "symfony-bundle",
    "description": "Symfony ApiCil Bundle",
    "keywords": ["referential", "json", "yaml", "api"],
    "license": "MIT",
    "authors": [
      {
        "name": "Louis-Arnaud CATOIRE",
        "email": "la.catoire@itefficience.com",
        "homepage": "https://www.itefficience.com"
    },
      {
        "name": "Benjamin BOUDIER",
        "email": "benjamin.boudier@idci-consulting.fr"
      }
    ],
    "require": {
      "php": ">=7.2",
      "eightpoints/guzzle-bundle": "^8.0",
      "symfony/dependency-injection": "^4.0 | ^5.0",
      "symfony/framework-bundle": "^4.0 | ^5.0",
      "symfony/options-resolver": "^4.0 | ^5.0",
      "symfony/property-info": "^4.0 | ^5.0",
      "symfony/serializer": "^4.0 | ^5.0",
      "ext-json": "*"
    },
    "autoload":{
      "psr-4":{
        "Mpp\\ApicilClientBundle\\": ""
      },
      "exclude-from-classmap": [
        "Resources/",
        "Tests/"
      ]
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5",
      "symfony/property-access": "^4.0 | ^5.0",
      "symfony/yaml": "^4.0 | ^5.0"
    }
  }
